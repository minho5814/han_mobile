<script type="text/javascript" src="../../js/common.js"></script>
<script>
$(window).load(function(){
	/* 결제방법 */
	$('.payment button').click(function(){
		$('.payment button').removeClass('on');
		$(this).addClass('on');
	});

	/* 인풋 셀렉트 레이어 타입 */
	$('.input-select input').mouseenter(function(){
		$(this).parents('.input-select').find('.list').show();
	});
	$('.input-select input').keyup(function(){
		var byte = $(this).val().length;
		$(this).parents('.input-select').find('.byte-text .byte').html(byte);
	});
	$('.input-select .list .item').click(function(){
		var txt = $(this).html();
		var byte = $(this).text().length;
		if($(this).hasClass('write')){
			$(this).parents('.input-select').find('input').val('').focus();
		}else{
			$(this).parents('.input-select').find('input').val(txt);
		}
		$(this).parents('.input-select').find('.list').hide();
		$(this).parents('.input-select').find('.byte-text .byte').html(byte);
	});
	$('.input-select').mouseleave(function(){
		$(this).find('.list').hide();
	});

	/* 장바구니 상세페이지 아코디언 기능 */
	$('.cart-wrap .section.on .cont-area').slideDown(200);
	$('.cart-wrap .section .btn-aco').click(function(){
		if($(this).parents('.section').hasClass('on')){
			$(this).parents('.section').removeClass('on').find('.cont-area').slideUp();
		}else{
			$(this).parents('.section').addClass('on').find('.cont-area').slideDown();
		}
	});

	/*툴팁 팝업*/
	/*출자금 자동증자금*/
	$('.btn-capital-increase').click(function(){
		$('.pop-capital-increase').fadeIn(200);
		$('.block-ui').fadeIn();
	});
});
</script>
